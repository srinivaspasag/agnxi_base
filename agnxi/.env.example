# Supabase (required for DB, Auth, Realtime)
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key

# QStash (queues; optional in dev)
QSTASH_TOKEN=
QSTASH_CURRENT_SIGNING_KEY=
QSTASH_NEXT_SIGNING_KEY=

# Upstash Redis (optional)
UPSTASH_REDIS_REST_URL=
UPSTASH_REDIS_REST_TOKEN=

# Agent execution (Cloudflare Workers / Fly.io)
AGENT_WORKER_URL=
AGENT_WORKER_SECRET=

# App URL (optional; on Vercel, VERCEL_URL is used automatically for worker callback)
NEXT_PUBLIC_APP_URL=http://localhost:3000

# Tenant limits (defaults; overridable per tenant in DB tenants.settings.limits)
LIMITS_MAX_AGENTS=50
LIMITS_MAX_CONCURRENT_INVOCATIONS=10
LIMITS_INVOCATION_TIMEOUT_SEC=300
LIMITS_MAX_REQUEST_BODY_BYTES=1048576
LIMITS_MAX_STORAGE_MB=100
LIMITS_MAX_API_KEYS=20

# Logging
LOG_LEVEL=info
